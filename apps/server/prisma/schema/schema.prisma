generator client {
  provider     = "prisma-client"
  output       = "../generated"
  moduleFormat = "esm"
  runtime      = "bun"
}

generator markdown {
  provider = "prisma-markdown"
  output   = "../docs/db.md"
  title    = "Database Schema"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
